@if (User != null)
{
    if (User.Gender == 0)
    {
        <MudIcon Icon="@Icons.Material.Outlined.Face5" Color="Color.Primary" />
    }
    else
    {
        <MudIcon Icon="@Icons.Material.Outlined.Face3" Color="Color.Error" />
    }
}

@code {
    [Parameter, EditorRequired] public UsersDto User { get; set; } = null!;
}
